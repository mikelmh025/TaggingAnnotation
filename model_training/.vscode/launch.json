    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Test Tag",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "args": [
                "--dataset=face_attribute",
                "--seed=0",
                "--result_dir=test_result",
                "--loss=mse",
                "--model=resnet34",
                "--batch_size=16",
                "--get_top_k=5",
                "--target_mode=tag",
                "--checkpoint=debug_dir/tagging/resnet/msebest.pt",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Test Img",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "args": [
                "--dataset=face_attribute",
                "--seed=0",
                "--result_dir=test_result",
                "--loss=mse",
                "--model=resnet50",
                "--batch_size=16",
                "--get_top_k=5",
                "--target_mode=img",
                "--checkpoint=debug_dir/direct_50/resnet/msebest.pt",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Train",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "args": [
                "--dataset=face_attribute",
                "--seed=0",
                "--result_dir=debug_dir",
                "--loss=mse",
                "--model=vit",
                "--lr=0.001",
                "--batch_size=4",
                "--debug",
                "--target_mode=img",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Test img",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "args": [
                "--dataset=face_attribute",
                "--seed=0",
                "--result_dir=debug_testing",
                "--loss=mse",
                "--model=resnet50",
                "--batch_size=16",
                "--debug",
                "--checkpoint=debug_dir/direct_50/resnet/msebest.pt",
                "--target_mode=img"
            ],
            "console": "integratedTerminal"
        },
    ]
}